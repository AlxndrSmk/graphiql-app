    "codemirror-graphql": "^2.0.10",
    "firebase": "^10.7.1",
    "firebase-admin": "^11.11.1",
    "graphql": "^16.8.1",
    "next": "14.0.3",
    "nookies": "^2.5.2",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.48.2",
    "react-router": "^6.20.1"
